@model DepartmentViewModel
@{
    ViewData["Title"] = "Create";
}

<div class="card border border-primary">
    <div class="card-header  bg-primary">
        <h2 class="text-white">Create New Department</h2>
    </div>
    <div class="card-body">


        <form asp-action="Create" method="post">
            <partial name="PartialViews/CreateEditPartialView" model="@Model" />


            <div asp-validation-summary="All"></div>
            <div class="mt-4 m-auto">
                <input class="btn btn-primary" type="submit" value="Create Department" />
                <a asp-action="Index" class="btn btn-outline-dark">Back to list</a>
            </div>
        </form>
    </div>
</div>



